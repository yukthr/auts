---
-
  connection: local
  gather_facts: false
  hosts: mx
  name: "This is to do a simple commit check! operation on Device"
  roles:
    - Juniper.junos
  tasks:
    -
      name: "commit check"
      juniper_junos_config:
        host={{ inventory_hostname }}
        username=lab
        passwd=lab123
        port=830
        load='set'
        src='final_config_2019-02-06T01:50:01.657498'
        commit=yes
        diff=True
      register: junos
    -
      name: "Output"
      debug:
              var: junos
    - local_action: copy content={{ junos | to_nice_json }} dest=output
